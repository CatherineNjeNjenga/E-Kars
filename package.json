{
  "name": "e-kars",
  "version": "1.0.0",
  "proxy": "https://e-kars.onrender.com/",
  "description": "Used electric cars",
  "main": "index.js",
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node backend/index.js",
    "server": "nodemon backend/index.js",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seederScript.js",
    "heroku-postbuild": " npm install --prefix frontend && npm run build --prefix frontend  ",
    "render-postbuild": " NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend  ",
  },
  "author": "Bryan Yi",
  "license": "ISC",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.2.0",
    "@stripe/stripe-js": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "redux-devtools-extension": "^2.13.8",
    "sendgrid-mail": "0.0.3",
    "stripe": "^8.131.1"
  }
}
